<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    >

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >

    <TextView
        android:id="@+id/textView6"
        style="@style/StyleText.header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/spacing_medium"
        android:text="나의 탄소 감축 활동"
         />



    <FrameLayout
        android:id="@+id/layer_myact_Container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        />

    <TextView
        android:id="@+id/textView"
        style="@style/StyleText.header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/spacing_medium"
        android:text="나의 인증활동"
        />

    <include
        android:id="@+id/certify_graph"
        layout="@layout/layer_graph_certify_group"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        />

    <TextView
        android:id="@+id/textView3"
        style="@style/StyleText.header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/spacing_medium"
        android:text="나의 특별활동"
        />

    <include
        android:id="@+id/special_graph"
        layout="@layout/layer_graph_special_card"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        />








</LinearLayout>
</ScrollView>
</layout>